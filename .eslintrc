{
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "no-console": 1,
    "no-underscore-dangle": 0,
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "react/prop-types": 0,
    "no-case-declarations": 0,
    "no-unused-vars": 1,
  }
}
